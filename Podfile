  # Pods for SplitWiser
  pod 'Firebase/Database'
  pod 'Firebase/Auth'
  pod 'FirebaseUI/Auth'
  pod 'FirebaseUI/Google'
  pod 'Firebase', '~> 4.7'
  pod 'ZAlertView', '~> 1.0'
  pod 'PHDiff', '~> 1.1'
  pod 'CountryKit', '~> 1.0'

  target 'SplitWiserTests' do
    inherit! :search_paths
    # Pods for testing
    
    pod 'Firebase/Database'
    pod 'Firebase/Auth'
    pod 'FirebaseUI/Auth'
    pod 'FirebaseUI/Google'
    pod 'Firebase', '~> 4.7'

  end

  target 'SplitWiserUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end

